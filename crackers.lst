00042352                            ; ---------------------------------------------------------------------------
00042352                            ; Object Positioning
00042352                            ; ---------------------------------------------------------------------------
00042352                            ;
00042352                            ; Format:	dc.w $XXXX,$YYYY,$AAAA,$FFFF
00042352                            ;
00042352                            ; XXXX 	=	Object's X Position (If FFFF is placed in X position then it's the end of the Object position file)
00042352                            ; YYYY	=	Object's Y Position
00042352                            ; AAAA	=	Object's ID
00042352                            ; FFFF	=	End of Object's position code (Unless the object is a pathswapper)
00042352                            ;
00042352                            ; ---------------------------------------------------------------------------
00042352                            ; List of Valid and Invalid Object ID's
00042352                            ;
00042352                            ; AAAA = changes exactly what object is being placed (This cannot be an odd value or the engine will crash)
00042352                            ;
00042352                            ; AAAA -> 00	=	Spring red Right
00042352                            ; AAAA -> 02	=	Spring red Up
00042352                            ; AAAA -> 04	=	Spring red Left
00042352                            ; AAAA -> 06	=	Blank
00042352                            ; AAAA -> 08	=	Spring red Up (Same as 02)
00042352                            ; AAAA -> 0A	=	Blank
00042352                            ; AAAA -> 0C	=	Spring red Down
00042352                            ; AAAA -> 0E	=	Blank
00042352                            ;
00042352                            ; AAAA -> 10	=	Blank
00042352                            ; AAAA -> 12	=	Spring red Angle Left/Up
00042352                            ; AAAA -> 14	=	Spring red Angle Right/Up
00042352                            ; AAAA -> 16	=	(Crashes)
00042352                            ; AAAA -> 18	=	Spring red Angle Left/Up
00042352                            ; AAAA -> 1A	=	(Crashes)
00042352                            ; AAAA -> 1C	=	Spring red Angle Down/Right
00042352                            ; AAAA -> 1E	=	(Crashes)
00042352                            ;
00042352                            ; AAAA -> 20	=	Spring red Angle Down/Left
00042352                            ; AAAA -> 22	=	(Crashes)
00042352                            ; AAAA -> 24	=	Blank
00042352                            ; AAAA -> 26	=	(Crashes)
00042352                            ; AAAA -> 28	=	A Ring that spawns when hit from spikes (Uncollectable)
00042352                            ; AAAA -> 2A	=	(Crashes)
00042352                            ; AAAA -> 2C	=	Blank
00042352                            ; AAAA -> 2E	=	Blank
00042352                            ;
00042352                            ; AAAA -> 30	=	Blank
00042352                            ; AAAA -> 32	=	(Crashes)
00042352                            ; AAAA -> 34	=	Spring yellow Right
00042352                            ; AAAA -> 36	=	Spring yellow Angle Right/Up
00042352                            ; AAAA -> 38	=	Spring yellow Left
00042352                            ; AAAA -> 3A	=	Spring yellow Angle Right/Up
00042352                            ; AAAA -> 3C	=	Spring yellow Up
00042352                            ; AAAA -> 3E	=	Spring yellow Angle Right/Up
00042352                            ;
00042352                            ; AAAA -> 40	=	Spring yellow Down
00042352                            ; AAAA -> 42	=	Spring yellow Angle Right/Up
00042352                            ; AAAA -> 44	=	Spring yellow Angle Right/Up
00042352                            ; AAAA -> 46	=	(Crashes)
00042352                            ; AAAA -> 48	=	Spring yellow Angle Left/Up
00042352                            ; AAAA -> 4A	=	(Crashes)
00042352                            ; AAAA -> 4C	=	Spring yellow Angle Down/Right
00042352                            ; AAAA -> 4E	=	(Crashes)
00042352                            ;
00042352                            ; AAAA -> 50	=	Spring yellow Angle Down/Left
00042352                            ; AAAA -> 52	=	Spikes Up
00042352                            ; AAAA -> 54	=	Spikes Up
00042352                            ; AAAA -> 56	=	Spikes Down (This seems to lag the engine and cause gliches after a few seconds of being present)
00042352                            ; AAAA -> 58	=	Spikes Down
00042352                            ; AAAA -> 5A	=	(Crashes)
00042352                            ; AAAA -> 5C	=	Spikes Right
00042352                            ; AAAA -> 5E	=	(Crashes)
00042352                            ;
00042352                            ; AAAA -> 60	=	Spikes Left
00042352                            ; AAAA -> 62	=	(Crashes)
00042352                            ; AAAA -> 64	=	Spikes Angle Right/Up
00042352                            ; AAAA -> 66	=	Spikes Angle Left/Up
00042352                            ; AAAA -> 68	=	Spikes Angle Left/Up
00042352                            ; AAAA -> 6A	=	Spikes Angle Down/Left
00042352                            ; AAAA -> 6C	=	Spikes Angle Down/Right
